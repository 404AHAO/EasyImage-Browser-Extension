{
  "name": "简单图床 - Edge Version",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "支持多文件上传的无数据库图床,最新html5自适应页面兼容手机电脑，简单方便支持一键复制，支持多域名，api上传。",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "icons": {
    "16": "images/image_icon_153794.png",
    "48": "images/image_icon_153794.png",
    "128": "images/image_icon_153794.png"
  },
  "browser_action": {
    "default_title": "简单图床浏览器上传插件 - Chrome Version"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "storage",
    "contextMenus",
    "<all_urls>"
  ],
  "options_page": "pages/options.html",
  "homepage_url": "https://png.cm/"
}